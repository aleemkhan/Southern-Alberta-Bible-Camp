<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sabcapp.sabc" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SABC</name>
    <description>
        Southern Alberta Bible Camp.
    </description>
    <author email="mr.aleem001@gmail.com" href="http://isonstudios.com">
        Abdul Aleem Khan
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-media" />
    <plugin name="cordova-plugin-device" />
    <plugin name="com.phonegap.plugins.pushplugin" spec="2.5.0" source="pgb" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
